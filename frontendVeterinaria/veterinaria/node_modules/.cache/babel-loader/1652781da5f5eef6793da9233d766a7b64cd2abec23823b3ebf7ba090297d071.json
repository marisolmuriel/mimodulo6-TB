      })\r\n        .catch(error=>{\r\n          console.log(error);\r\n        })\r\n      },\r\n      propietario(){\r\n        this.axios({\r\n          method: 'get',\r\n          url: 'http://localhost:8000/veterinaria/propietario/'+this.$route.params.id       \r\n        })\r\n        .then(response=>{\r\n          this.datospropietario=response.data;\r\n          console.log(response);\r\n        })\r\n        .catch(error=>{\r\n           console.log(error);\r\n        })\r\n      },\r\n    },\r\n    created() {\r\n      this.propietario();\r\n    },\r\n    mounted() {\r\n      this.listado();\r\n    },\r\n  }\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style >\r\n#principal{\r\n  background: url(../assets/imagenes/tapiz1.jpg);\r\n  background-attachment: scroll;\r\n  background-repeat: repeat;\r\n \r\n \r\n\r\n}\r\n\r\n#contdatos {\r\n\tbackground-color:  rgba(217,255,190,1);\r\n\twidth: 90%;\r\n\theight: auto;\r\n\tmargin-left: 5%;\r\n } \r\n\r\n#datosprop {\r\n\tbackground-color:  rgba(217,255,190,1);\r\n\twidth: 90%;\r\n\theight: auto;\r\n\tmargin-left: 5%;\r\n } \r\n  img {\r\n                max-width: 100%;\r\n                }\r\n</style>\r\n"],"mappings":"AAqDE,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,QAAQ,EAAC,EAAE;MACXC,gBAAgB,EAAC;QACfC,OAAO,EAAC,EAAE;QACVC,SAAS,EAAC,EAAE;QACZC,EAAE,EAAC,EAAE;QACLC,SAAS,EAAC,EAAE;QACZC,QAAQ,EAAC,EAAE;QACXC,MAAM,EAAC,EAAE;QACTC,IAAI,EAAC;MACP;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAOA,CAAA,EAAE;MACP,IAAI,CAACC,KAAK,CAAC;QACTC,MAAM,EAAC,KAAK;QACZC,GAAG,EAAC,yDAAyD,GAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC;MACnF,CAAC,EACAC,IAAI,CAACC,QAAQ,IAAE;QACd,IAAI,CAAClB,QAAQ,GAACkB,QAAQ,CAACnB,IAAI;QAC3BoB,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;MACvB,CAAC,EACAG,KAAK,CAACC,KAAK,IAAE;QACZH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;MACpB,CAAC;IACH,CAAC;IACDC,WAAWA,CAAA,EAAE;MACX,IAAI,CAACZ,KAAK,CAAC;QACTC,MAAM,EAAE,KAAK;QACbC,GAAG,EAAE,gDAAgD,GAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CA